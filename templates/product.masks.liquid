          <h3 class="price compare-at" style="text-align: left">{{ product.compare_at_price | money }}</h3>
          {% endif %}
        </div>



        <style>
          #quantity {
            padding: 5px;
            width: 300px;
            border: 1px solid #555;
            color: black;
          }
        </style>


        <form action="/cart/add" method="post">
 <input type="hidden" name="id" value="{{ product.variants.first.id }}" />
          {% unless product.available %}
          <!--           <p class="btn otl">Sold Out</p> -->
          {% else %}
          <div class="cart-flex">
            <div class="rounded-qty" style="margin-bottom: 20px">
              <div class="num-in">
                <span class="minus dis">-</span>
                <input readonly class="in-num" value="1" min="1" type="number" name="quantity" style="color: black" >
                <span class="plus dis">+</span>
              </div>
            </div>
            <input type="submit" class="btn blue" value="Buy Now" />
          </div>

          {% unless product.available %}
          {% else %}
          <div class="shipping-info"><i class="fas fa-shipping-fast"></i> Free Shipping</div>
          {% endunless %}
          <div class="shipping-info"><i class="fas fa-check"></i> FDA Registered</div>

          <div class="price-chart">

            <div class="price-item rowhead"><p class="qty-break"># of Packs</p><div class="price-matched rowhead">Price</div></div>
            <div class="price-item"><p class="qty-break">1-9</p><div class="price-matched"> <strike>$45.00</strike><strong>$20.00</strong></div></div>
            <div class="price-item"><p class="qty-break">10-99</p><div class="price-matched"><strike>$42.50</strike><strong>$19.00</strong> </div></div>
            <div class="price-item"><p class="qty-break">100-999</p><div class="price-matched"><strike>$37.50</strike><strong>$17.50</strong> </div></div>
            <div class="price-item"><p class="qty-break">1,000+</p><div class="price-matched"><strike>$30.00</strike><strong>$15.00</strong> </div></div>
            <div class="price-item rowhead"><p class="qty-break">*discount applied at checkout</p></div>
          </div>


          {% endunless %}

        </form>


      </div>
    </div>

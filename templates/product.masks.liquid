  <script>

    if ($(window).width() > 600) {
      $('.welp' ).wrap ("<div class='product-photo-container'></div>");
    }

    else {
      $('.welp' ).wrap ("<a id='fix'></a>");
    }

    $('.num-in span').click(function () {
      var $input = $(this).parents('.rounded-qty').find('input.in-num');
      var count = $input.val();
      var newVal = 0;

      if($(this).hasClass('minus')) {
        console.log(count);
        if(count > 0) {
          newVal = parseFloat(count) - 1;
        }
      }
      else  {
        newVal = parseFloat(count) + 1;
      }
      $input.val(parseFloat(newVal));
    });



  </script>

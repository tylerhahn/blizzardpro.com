        {% endif %}
        <h1 class="desktop">{{ product.title }}</h1>
        <h2 class="product-subtext desktop"> {{ product.metafields.c_f.Product_Subtitle }}</h2>

        <div class="price-container">
          <h3 class="price" style="text-align: left"> {{ product.price_max | money }} {% if product.tags contains 'msrp' %}<span>MSRP</span>{% endif %}</h3>

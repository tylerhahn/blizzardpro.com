<!-- Recommend Collection -->
<div class="recommended">
  <h2>You May Also Like</h2>
  {% if settings.how-recommended != blank %} {% if
  collections[settings.how-recommended].products_count != 0 %}
  <div class="grid-container">
    {% for product in collections[settings.how-recommended].products limit: 8 %}
    <div class="recommended-item">
      <img
        src="{{ product.featured_image | img_url: 'master' }}"
        alt="{{ product.title }}"
      />
      <div class="info">
        <h3>{{ product.title }}</h3>
        <a href="{{ product.url }}" class="btn btn-blue">View</a>
      </div>
    </div>
    {% endfor %} {% endif %} {% endif %}
  </div>
</div>


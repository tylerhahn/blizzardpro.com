<!-- Hero How -->


<section id="hero">

{% include 'rotator-how' %}


</section>


<div class="product-carousel">

	<div class="blurred"></div>

	<div class="center" data-aos="fade-up">

      {% for product in collections.how-landing-page-lineup.products %}


   

		<div class="item-c">
			<div class="container">
                <img src="{{ product | img_url: 'large' }}" alt="{{ product.featured_image.alt | escape }}" />
				<h3>{{ product.title }}</h3>
				<p>{{ product.metafields.c_f.Product_Subtitle }}</p>
				<div class="carousel-button"><a class="btn btn-blue" href="{{ product.url }}">Learn More</a>
              </div>
			</div>
		</div>



      {% endfor %}


	</div>
<!-- 	<a class="btn btn-blue cnt" href="/">All Product</a> -->

</div></section>




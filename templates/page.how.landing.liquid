<!-- Two Col Blog Right -->
<div id="{{setting}}" class="two-col-blog">
  <div class="flex-container right-text">
    <div class="left">
      <h2>{{ settings.how-blog-title }}</h2>
      <p>{{ settings.how-blog-excerpt }}</p>
      <a href="{{ settings.how-blog-button-url }}" class="btn btn-blue"
        >{{ settings.how-blog-button-title }}</a
      >
    </div>
    <div class="right">
      <img
        src="{{ settings.how-blog-image | img_url: 'master' }}"
        alt="Case Study"
      />
      <div class="caption">{{ settings.how-blog-pullquote }}</div>
    </div>
  </div>
</div>

<!-- Hero Text Feature -->
<div
  class="hero-text-feature"
  style="background-image: url({{ settings.how-tf-image | img_url: 'master' }})"
>
  <div class="caption-container">
    <h3>{{ settings.how-tf-subtitle }}</h3>
    <h2>{{ settings.how-tf-title }}</h2>
    <p>{{ settings.how-tf-p }}</p>
  </div>
</div>

<!-- Hero Simple Right Image -->

{% assign product1 = all_products[settings.how-prod-1] %}

<div class="hero-simple">
  <div class="flex-container reverse">
    <div class="left">
      <h3>{{ product1.title }}</h3>
      <h2>{{ product1.metafields.c_f.Landing_Page_Copy }}</h2>
      <a href="{{ product1.url }}" class="btn btn-blue"
        >Learn More</a
      >
    </div>
    <div class="right">
      <img
        src="{{ product1.images[0].src | img_url: 'master' }}"
        alt="{{ product1.title }}"
      />

    </div>
  </div>
</div>

<div class="product-block-slider">

    <div data-fancy="gallery" href="{{ product1.images[3].src | img_url: 'master' }}" class="hero-slider-item" style="background-image: url( {{ product1.images[3].src | img_url: 'master' }} )"></div>
    <div data-fancy="gallery" href="{{ product1.images[4].src | img_url: 'master' }}" class="hero-slider-item" style="background-image: url( {{ product1.images[4].src | img_url: 'master' }} )"></div>
    <div data-fancy="gallery" href="{{ product1.images[5].src | img_url: 'master' }}" class="hero-slider-item" style="background-image: url( {{ product1.images[5].src | img_url: 'master' }} )"></div>
    <div data-fancy="gallery" href="{{ product1.images[6].src | img_url: 'master' }}" class="hero-slider-item" style="background-image: url( {{ product1.images[6].src | img_url: 'master' }} )"></div>
    <div data-fancy="gallery" href="{{ product1.images[7].src | img_url: 'master' }}" class="hero-slider-item" style="background-image: url( {{ product1.images[7].src | img_url: 'master' }} )"></div>

</div>




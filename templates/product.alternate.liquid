<style>
	input[type="submit"] {
		background: white;
		color: #4187f9;
		border: 1px solid;
		padding: 10px;
		border-radius: 25px;
		font-size: 12px;
		letter-spacing: 4px;
		text-transform: uppercase;
		margin-top: 30px;
		transition: .5s;
		font-family: "AvantGarde Bold";
		width: 210px;
	}

	#navigate {
		background: black;
	}

  div#inline {
    padding: 10px;
}




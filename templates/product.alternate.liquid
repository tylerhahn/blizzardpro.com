			<img class="lazy welp" src="{{ product | img_url: 'grande' }}" alt="{{ product.featured_image.alt | escape }}" /> {% if product.metafields.c_f.Youtube_URL %}
			<div class="btn-group">

{% if product.tags contains 'discontinued'%}

              {% else %}
				<a class="btn btn-outline gray" href="#modal-one">Register</a>
              {% endif %}

